<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.destresstreatment">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".ShowMoreMemes"></activity>
        <activity android:name=".PsychAdapter" />
        <activity android:name=".Psychiatrist" />
        <activity android:name=".UpdatePreferences" />
        <activity android:name=".DisplayMeme" />
        <activity android:name=".MemeAdapter" />
        <activity android:name=".MemeRecommendations" />
        <activity android:name=".PlayMusic" />
        <activity android:name=".MusicRecommendations" />
        <activity android:name=".RecommendationsInterface" />
        <activity android:name=".StressCauseIdentification" />
        <activity android:name=".UpdateExperiences" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>